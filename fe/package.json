        "@mui/x-data-grid": "^6.4.0",
        "@types/node": "20.1.4",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "autoprefixer": "10.4.14",
        "axios": "^1.4.0",
        "bufferutil": "^4.0.7",
        "clsx": "^1.2.1",
        "dayjs": "^1.11.7",
        "eslint": "8.40.0",
        "eslint-config-next": "13.4.2",
