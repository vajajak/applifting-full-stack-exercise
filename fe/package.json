{
    "name": "applifting-full-stack-exercise",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3001",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "graphql-codegen": "env-cmd -f ../.env graphql-codegen --config codegen.yml",
